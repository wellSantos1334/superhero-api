{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Jest Debug",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/node_modules/.bin/jest",
        "args": ["--runInBand", "--config", "jest.e2e.config.ts", "--detectOpenHandles"],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
}
